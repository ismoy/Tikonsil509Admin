<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Tikonsil509Admin" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/purple_700</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="PBarTheme" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="colorSecondary">@color/purple_700</item>
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
    </style>
    <style name="Circular">
        <item name="cornerSize">50%</item>

    </style>
    <style name="CustomShip" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="chipBackgroundColor">@color/bg_color</item>
        <item name="android:textColor">@color/chip_text</item>
        <item name="checkedIconTint">@color/white</item>
        <item name="rippleColor">@color/whiteAlpha</item>

    </style>
    <style name="SplashScreen" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_screen</item>
    </style>
    <style name="BtuttonCancelTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorSecondary">@color/colorerror</item>
        <item name="colorPrimary">@color/colorerror</item>
    </style>
</resources>